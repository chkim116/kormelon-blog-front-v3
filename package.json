{
  "name": "kormelon-blog-front-v3",
  "engines": {
    "node": ">=18.16.0"
  },
  "version": "2.0.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint & cross-env NODE_ENV=production next build",
    "build:dev": "cross-env NODE_ENV=development next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "postbuild": "cross-env NODE_ENV=production next-sitemap",
    "sb": "storybook dev -p 6006",
    "build-sb": "storybook build",
    "type-check": "tsc --noEmit --incremental",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:push-dev": "dotenv -e .env.development -- npx prisma db push",
    "db:pull-dev": "dotenv -e .env.development -- npx prisma db pull",
    "db:migrate-dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "db:push-prod": "dotenv -e .env.production -- npx prisma db push",
    "db:pull-prod": "dotenv -e .env.production -- npx prisma db pull",
    "db:migrate-prod": "dotenv -e .env.production -- npx prisma migrate dev",
    "studio:prod": "dotenv -e .env.production -- npx prisma studio"
  },
